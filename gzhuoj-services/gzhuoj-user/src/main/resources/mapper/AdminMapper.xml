<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gzhuoj.usr.mapper.UserMapper">

    <select id="pageUserPrivilege" parameterType="com.gzhuoj.usr.dto.req.AdminPrivilegeListReqDTO" resultType="com.gzhuoj.usr.model.entity.UserDO">
        select *
        from user
        <where>
            <if test="search != null and search != ''">
                AND (user_account like concat('%', #{search}, '%') OR username like concat('%', #{search}, '%'))
            </if>
        </where>
    </select>
</mapper>